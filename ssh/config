ForwardAgent True

Host dev*
    HostName %h.meraki.com
    User jellingt

Host sdg0 dal0 fra0 syd0
    HostName %h.meraki.com
    User jellingt
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h:%p
    ControlPersist yes

Host phone phone-dev phone-spare n* dal1* dal2* dal3* sdg1* sdg2* sdg3* mun1* fra1* sf*
    Hostname %h.meraki.com
    User jellingt
    ProxyCommand ssh sdg0 -W %h:%p

Host sf*
    Hostname %h.meraki.com
    User jellingt
